    "# 연도 기준 groupby()\n",
    "reason_not_read_gr = reason_not_read.groupby('연도')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "dfcc6fc5",
   "metadata": {},
   "outputs": [],
   "source": [
    "# 함수 정의 : 독서장애요인 1순위에 가중치 2를 부여, 합산한 값 구하기.\n",
    "def func_reason_not_read (gr) :\n",
    "    gr.replace({9:np.nan,0:np.nan},inplace=True)\n",
    "    gr.dropna(how='any',inplace=True)\n",
    "    return gr['독서장애요인1'].value_counts()*2 + gr['독서장애요인2'].value_counts()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "9f1792e7",
   "metadata": {},
   "outputs": [],
   "source": [
    "# 그룹화 함수 적용\n",
    "reason_not_read = reason_not_read_gr.apply(func_reason_not_read)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "66733132",
   "metadata": {},
   "outputs": [],
   "source": [
    "# 다중인덱스 제거\n",
    "reason_not_read = reason_not_read.unstack(level=1)\n",
    "reason_not_read.fillna(0,inplace=True)\n",
    "# 시리즈 형태를 데이터 프레임으로 변경\n",
    "reason_not_read =pd.DataFrame(reason_not_read)"
   ]
