   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "431118c5",
   "metadata": {},
   "outputs": [],
   "source": [
    "#2019,2021년 종류별 독서시간 -> 전체 독서시간\n",
    "for i in range(1,3):\n",
    "    read_avg_stu.iloc[i,0] = read_avg_stu.iloc[i,2] + read_avg_stu.iloc[i,4] + read_avg_stu.iloc[i,6]\n",
    "    read_avg_stu.iloc[i,1] = read_avg_stu.iloc[i,3] + read_avg_stu.iloc[i,5] + read_avg_stu.iloc[i,6]\n",
    "    \n",
    "read_avg_stu"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "8f5e4adc",
   "metadata": {},
   "outputs": [],
   "source": [
    "# 종류별 평일/주말 평균 독서시간 -> 일 평균 독서시간 추출\n",
    "read_avg_stu.insert(2,'일 평균 독서시간(분)',(read_avg_stu.iloc[:,0]*5 + read_avg_stu.iloc[:,1]*2)/7)\n",
    "read_avg_stu.insert(5,'종이_일 평균 독서시간(분)',(read_avg_stu.iloc[:,3]*5 + read_avg_stu.iloc[:,4]*2)/7)\n",
    "read_avg_stu.insert(8,'전자_일 평균 독서시간(분)',(read_avg_stu.iloc[:,6]*5 + read_avg_stu.iloc[:,7]*2)/7)\n",
    "read_avg_stu.insert(11,'오디오_일 평균 독서시간(분)',(read_avg_stu.iloc[:,9]*5 + read_avg_stu.iloc[:,10]*2)/7)\n",
